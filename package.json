{
  "name": "kevas",
  "version": "4.0.0",
  "description": "Stream replacing {{keys}} with values",
  "main": "lib/index.js",
  "bin": "bin/kevas.js",
  "author": "Eli Doran <eli+npm@elidoran.com>",
  "license": "MIT",
  "keywords": [
    "transform",
    "kevas",
    "key",
    "keys",
    "value",
    "values",
    "stream",
    "replace",
    "low memory",
    "mustache",
    "template",
    "handlebars",
    "mustache"
  ],
  "scripts": {
    "lint": "coffeelint -f build/coffeelint.json lib",
    "build": "coffee --bare --compile --no-header lib bin",
    "clean": "rm -rf lib/*.js bin/command.js",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "test": "_mocha --compilers coffee:coffee-script/register --reporter spec --bail --check-leaks test/lib test/bin",
    "test4": "nave use 4.8 npm test",
    "test6": "nave use 6.10 npm test",
    "test7": "nave use 7.10 npm test",
    "test-all": "npm run test4 && npm run test6 && npm run test7",
    "precoverage": "npm run clean",
    "coverage": "istanbul cover -x 'benchmark/**' -x 'build/**' -x 'docs/**' -x 'examples/**' _mocha -- --compilers coffee:coffee-script/register --require build/coverage.js -R spec test/lib test/bin",
    "precoverage-lcov": "npm run clean",
    "coverage-lcov": "istanbul cover -x 'benchmark/**' -x 'build/**' -x 'docs/**' -x 'examples/**' _mocha --report lcovonly -- --compilers coffee:coffee-script/register --require build/coverage.js -R spec test/lib test/bin",
    "devrun": "coffee bin/kevas.js --"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elidoran/node-kevas.git"
  },
  "bugs": "http://github.com/elidoran/node-kevas/issues",
  "devDependencies": {
    "benchmarked": "^1.1.1",
    "buffed": "^0.1.0",
    "chain-builder": "^0.13.2",
    "chalk": "^1.1.3",
    "coffee-coverage": "^2.0.1",
    "coffee-script": "^1.12.6",
    "coffeelint": "^1.16.0",
    "comma-number": "^2.0.0",
    "coveralls": "^2.13.1",
    "fspath": "^0.15.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.1",
    "mocha-lcov-reporter": "^1.3.0",
    "pad": "^1.1.0",
    "stating": "^0.4.1",
    "stream-search-helper": "^0.7.1",
    "streaming-format": "^1.1.0",
    "strung": "^1.1.6",
    "through2": "^2.0.3"
  },
  "dependencies": {
    "nuc": "^0.4.0",
    "value-store": "^0.3.6"
  }
}
